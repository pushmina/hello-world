#include <iostream>
#include<fstream>


using namespace std;
int matrix[10][10]={0,3,1,7,5,9,8,6,4,2,7,0,9,2,1,5,4,8,6,3,4,2,0,6,8,7,1,3,5,9,1,7,5,0,9,8,3,4,2,6,6,1,2,3,0,4,5,9,7,8,3,6,7,4,2,0,9,5,8,1,5,8,6,9,7,2,0,1,3,4,8,9,4,5,3,6,2,0,1,7,9,4,3,8,6,1,7,2,0,5,2,5,8,1,4,3,6,7,9,0};
void READ (const char* fileName);
const int N=20;
int COUNT (int num);
int x[N];

int main()
{
    ofstream fout("output.dat");
    READ("input.dat");
    for (int i=0; i<N; i++)
    {
        int d=10000;
        int x_ch=x[i];
        while (COUNT(x_ch)>0)
        {
            x_ch=x[i];
            int buf1=x_ch/d;
            x_ch%=d;
            d/=10;
            int buf2=x_ch/d;
            x_ch=(x_ch%d)+(buf1*d)+(buf2*d*10);
        }
        fout<<x_ch<<endl;
    }
    return 0;
}
void READ (const char* fileName)
{
    fstream file (fileName);
    if (!file)
        cout<<"error"<<endl;
    ifstream fin(fileName);
    for (int i=0; i<N; i++)
        fin>>x[i];
}

int COUNT (int num)
{
    int d=10000;
    int inter_val=0;
    for (int j=0; j<5; j++)
    {
        inter_val=(matrix[inter_val][num/d]);
        num%=d;
        d/=10;
    }
    return inter_val;
}
